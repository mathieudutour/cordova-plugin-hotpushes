<?xml version="1.0" encoding="UTF-8" ?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" id="cordova-plugin-hotpushes" version="0.1.0">

  <name>Hot Pushes</name>

  <description>
    A simple hot code push for ios and android.
  </description>

  <author>Mathieu Dutour</author>

  <license>MIT</license>

  <keywords>Hot pushes</keywords>

  <repo>https://github.com/mathieudutour/cordova-plugin-hotpushes.git</repo>

  <issue>https://github.com/mathieudutour/cordova-plugin-hotpushes/issues</issue>

  <dependency id="com.phonegap.contentsync" url="https://github.com/phonegap/phonegap-plugin-contentsync" commit="58040c2cceb3b1cb71e1a367603bbf6d2a0ee70e" />

  <js-module src="www/index.js" name="HotPush">
    <clobbers target="window.HotPush" />
    <clobbers target="window.hotPushJSONP" />
  </js-module>
</plugin>
